{
  "name": "joomla-to-markdown",
  "version": "0.0.2",
  "description": "Convert JSON Exported Joomla article content to markdown files",
  "author": "John M. Wargo @johnwargo",
  "bin": {
    "j2md": "./bin/run"
  },
  "homepage": "https://github.com/johnwargo/joomla-to-md",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "johnwargo/joomla-to-md",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.6",
    "chalk": "^5.1.2",
    "date-fns": "^2.29.3",
    "fs-extra": "^10.1.0",
    "turndown": "^7.1.1",
    "yesno": "^0.4.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.10",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.3",
    "@types/turndown": "^5.0.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3.2.28",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "j32md",
    "dirname": "j32md",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/johnwargo/joomla3-2-md/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
